[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=landing
Group=landing
WorkingDirectory=/srv/landing
ExecStart=/srv/landing/.venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/srv/landing/gunicorn.sock \
          landing_site.wsgi:application

[Install]
WantedBy=multi-user.target
